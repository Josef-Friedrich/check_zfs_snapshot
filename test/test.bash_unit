#! /bin/sh

. ./lib/test-helper.sh

test_get_last_snapshot() {
	source_exec ../check_zfs_snapshot
	mock_path bin
	NOW=$(date +%s)
	[ $(_get_last_snapshot dataset) -gt $((NOW - 1000)) ] || fail
}

